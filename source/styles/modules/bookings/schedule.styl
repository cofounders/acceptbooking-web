#app .bookings.schedule
	> a
		font-weight 700
		text-decoration none
		margin 5px
		padding 10px 0 13px 0
		display block
		text-align center
		border-radius 4px
		background-color #f0f1f2
		border 0.5px solid #c5cacb
		box-shadow inset 0px 0px 2px 0px rgba(0, 0, 0, 0.1),
			0px 0.5px 0px 0px rgba(255, 255, 255, 0.8)
		color #b7b9bb
		font-size 15px
		text-shadow 0 1px rgba(255, 255, 255, 0.9)
		vertical-align baseline
		&:before
			background-image url('/img/icn-addnew.png')
			background-repeat no-repeat
			background-size 13px
			content ' '
			display inline-block
			height 13px
			width 13px
			padding 0 6px 3px 0
			vertical-align bottom
