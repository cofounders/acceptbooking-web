#app .geocodes.search
	> h3
		margin 0
		border-top 1px solid #c5cacb
		color #666
		font-size 12px
		font-weight 700
		width 100%
		box-shadow 0 1px 2.5px rgba(0, 0, 0, 0.29),
			inset 0 1px 0 #fff
		background-image linear-gradient(bottom, mix(#dadada, rgba(246, 246, 246, 0.69)) 0%, mix(#f8f8f8, rgba(246, 246, 246, 0.69)) 100%)
		text-transform uppercase
		padding 3px 0 2.5px 10px
		display inline-block
		box-sizing border-box
		> i
			font-style normal
	> ol
		list-style-type none
		padding 0
		margin 0
		> li
			border-bottom 1px solid rgba(0, 0, 0, 0.05)
			height 40px
			&:last-of-type
				border-bottom-width 0
			> a
				display block
				height 100%
				width 100%
				line-height 40px
				text-decoration none
				font-size 12px
				font-weight 700
				color #333
				padding 0 0 0 10px
